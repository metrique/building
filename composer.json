{
    "name": "metrique/building",
    "description": "A flexible page manager for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Hudson",
            "email": "support@metrique.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Metrique\\Building\\": "src",
            "Metrique\\Building\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Metrique\\Building\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "php": "^7.0|^8.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.4"
    },
    "require": {
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Metrique\\Building\\BuildingServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
